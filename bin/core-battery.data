define Trial(stim,thresh,practice,statustext)
{


   ClearEventLoop()
   time0 <- GetTime()
   ##add the fixation right away.

   order <- Sample([1,-1])


   #default use the given fieldsize
   fieldsize <- gParams.fieldsize
   border <- gParams.border


   totalwidthneeded <- gParams.fieldsize*2+gParams.border*2
   if(totalwidthneeded>gVideoWidth)
   {
     over <- totalwidthneeded - gVideoWidth
     if(over>(2*border-20))
     {
       border <- 10
       totalwidthneeded <- gParams.fieldsize*2+border*2
       over <- totalwidthneeded-gVideoWidth
     }

   if(over>0)
   {
      fieldsize <- fieldsize - over/2
   }

   }




    ycenter <- 50 + border/2 + fieldsize/2
    xcenter1 <- gVideoWidth/2-border/2-fieldsize/2
    xcenter2 <- gVideoWidth/2+border/2+fieldsize/2


    header <- EasyLabel(gStrings.header,gVideoWidth/2,22,gWin,30)
    fCorr <- MakeFont(gPEBLBaseFont,0,30,MakeColor("green"),MakeColor("black"),0)
    fInc <- MakeFont(gPEBLBaseFont,0,30,MakeColor("red"),MakeColor("black"),0)
    
    feedback_corr <-MakeLabel(gStrings.correct,fCorr)
    feedback_inc <- MakeLabel(gStrings.incorrect,fInc)
    AddObject(feedback_corr,gWin)
    AddObject(feedback_inc,gWin)
    Move(feedback_corr,gVideoWidth/2,50)
    Move(feedback_inc,gVideoWidth/2,50)
    Hide(feedback_corr)
    Hide(feedback_inc)
    
    footer <- EasyLabel(gStrings.FooterL+"          "+gStrings.FooterR,gVideoWidth/2,ycenter+fieldsize/2+30,gWin,40)
    status <- EasyLabel(statustext,gVideoWidth/2,ycenter+fieldsize/2+70,gWin,16)
   if(order==1)
      {  ##higher on right


       ##backoff the size of the points  #xxyy
       layoutl <- LayoutPoints(xcenter1-fieldsize/2+gParams.dotradius,
                               xcenter1+fieldsize/2-gParams.dotradius,
			       ycenter-fieldsize/2+gParams.dotradius,
                               ycenter+fieldsize/2-gParams.dotradius,
			       Floor(stim+thresh))

       layoutR <- LayoutPoints(xcenter2-fieldsize/2+gParams.dotradius,
                               xcenter2+fieldsize/2-gParams.dotradius,
			       ycenter-fieldsize/2+gParams.dotradius,
                               ycenter+fieldsize/2-gParams.dotradius,
			       Floor(stim))



     } else {

       ##backoff the size of the points  #xxyy
       layoutl <- LayoutPoints(xcenter1-fieldsize/2+gParams.dotradius,
                               xcenter1+fieldsize/2-gParams.dotradius,
			       ycenter-fieldsize/2+gParams.dotradius,
                               ycenter+fieldsize/2-gParams.dotradius,
			       Floor(stim))

       layoutR <- LayoutPoints(xcenter2-fieldsize/2+gParams.dotradius,
                               xcenter2+fieldsize/2-gParams.dotradius,
			       ycenter-fieldsize/2+gParams.dotradius,
                               ycenter+fieldsize/2-gParams.dotradius,
			       Floor(stim+thresh))



     }

	 back1 <-Rectangle(xcenter1,ycenter, fieldsize,fieldsize,MakeColor("darkgrey"),1)
  	 back2 <-Rectangle(xcenter2,ycenter, fieldsize, fieldsize,MakeColor("darkgrey"),1)



	 AddObject(back1,gWin)
	 AddObject(back2,gWin)
	 dots <- []
	 loop(i,Merge(layoutl,layoutr))
         {

	      dot <-  Circle(First(i),Second(i),gParams.dotradius,MakeColor(gParams.dotcolor),gParams.dotfilled)
	      AddObject(dot,gWin)
	      dots <- Append(dots,dot)
         }

        timeb <- GetTime()
        elapsed <- timeb-time0
	##make the timing pre-presentation more similar
        Wait(200-elapsed)

	Draw()
    time2 <- GetTime()
    gStimOn <- 1

    if(gParams.prestime>0)
    {
      RegisterEvent("<TIMER>", 1, time2+gParams.prestime,"<GEQ>","HIDEME", [dots])
    }
	resp <- WaitForListKeyPress(["<lshift>","<rshift>"])
	time3 <- GetTime()

    corr <- ((order ==-1) and (resp == "<rshift>")) or
            ((order ==1) and (resp == "<lshift>"))
    rt <- time3-time2

   gRunningCorr <- Append(gRunningCorr,corr)
   if(Length(gRunningcorr)>10)
   {
     gRunningcorr <- SubList(gRunningCorr,2,11)
   }

     if(corr)
      {
            Show(feedback_corr)
	    Draw()
#	    Wait(500)
#	    Hide(feedback_corr)
#	    Draw()
            } else {

            Show(feedback_inc)
	    Draw()
#	    Wait(500)
#	    Hide(feedback_inc)
#           Draw()
            }
	Show(header)
	Draw()

     if(gParams.UseAudioFeedback)
      {
       if(corr)
        {
          PlayForeground(gCorrectSound)
        }else{
          PlayForeground(gIncorrectSound)
        }
      }else{
	Wait(400)
      }


   Draw()
   dataline <- gSubNum + ","+ gBlock + "," +gTrial+","+practice + ","+  stim +","+thresh+","+order+","+
   corr + ","+time2+","+rt

   FilePrint(gFileOut,dataline)
   FilePrint(gPooledFileOut,dataline)

   PushOnEnd(gRTs,rt)
   PushOnEnd(gCorr,corr)

   RemoveObjects(dots,gWin)

   ##In this staircase, we have a probability of staying regardless of
   ##correct/incorrect.  Set to 0 if you want to ignore it.
   if(corr)
      {
     newthresh <- Max([1,(thresh +gParams.correctAdj)])
   } else {
     newthresh <- thresh+gParams.errorAdj
   }

 return newthresh
}




