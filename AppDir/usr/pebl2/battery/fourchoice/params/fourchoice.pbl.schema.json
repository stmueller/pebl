{
  "test": "fourchoice",
  "version": "1.0",
  "description": "FOURCHOICE Test",
  "parameters": [
    {
      "name": "usesound",
      "type": "boolean",
      "default": 0,
      "description": "Whether audio feedback should be given. 1=yes, 0=no",
      "options": [
        0,
        1
      ]
    },
    {
      "name": "numtrials",
      "type": "integer",
      "default": 50,
      "description": "Number of trials to run."
    },
    {
      "name": "isi",
      "type": "integer",
      "default": 200,
      "description": "Inter-stimulus interval, in ms"
    },
    {
      "name": "responsetype",
      "type": "integer",
      "default": 2,
      "description": "Response method.  0=ask in task, 1= keyboard FJVN. 2=keypad"
    }
  ]
}