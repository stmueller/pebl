{
  "test": "dexterity",
  "version": "1.0",
  "description": "DEXTERITY Test",
  "parameters": [
    {
      "name": "Tolerance",
      "type": "integer",
      "default": 5,
      "description": "How close you have to get to the target."
    },
    {
      "name": "CursorSize",
      "type": "integer",
      "default": 5,
      "description": "Size of the cursor"
    },
    {
      "name": "Drift",
      "type": "float",
      "default": 0.3,
      "description": "drift back toward center"
    },
    {
      "name": "MinDist",
      "type": "integer",
      "default": 100,
      "description": "Minimum distance to target"
    },
    {
      "name": "MaxDist",
      "type": "integer",
      "default": 280,
      "description": "Maximum distance to target"
    },
    {
      "name": "timeOut",
      "type": "integer",
      "default": 10000,
      "description": "Maximum time allowed to hit target"
    },
    {
      "name": "minnoise",
      "type": "boolean",
      "default": 1,
      "description": "smallest noise condition",
      "options": [
        0,
        1
      ]
    },
    {
      "name": "maxnoise",
      "type": "integer",
      "default": 20,
      "description": "largest noise condition"
    },
    {
      "name": "noisestep",
      "type": "integer",
      "default": 3,
      "description": "stepsize between smallest and largest nose"
    },
    {
      "name": "trialspercond",
      "type": "integer",
      "default": 10,
      "description": "number of trials per noise condition"
    }
  ]
}