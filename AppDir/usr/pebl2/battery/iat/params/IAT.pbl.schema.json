{
  "test": "IAT",
  "version": "1.0",
  "description": "IAT Test",
  "parameters": [
    {
      "name": "PosWordtitle",
      "type": "string",
      "default": "Verbs",
      "description": "Title of 'positive' word dimension"
    },
    {
      "name": "NegWordtitle",
      "type": "string",
      "default": "Adjectives",
      "description": "Title of 'negative' word dimension"
    },
    {
      "name": "pospictitle",
      "type": "string",
      "default": "Living things",
      "description": "Title of 'positive' picture dimension"
    },
    {
      "name": "negpictitle",
      "type": "string",
      "default": "Man-made things",
      "description": "Ttile of 'negative' picture dimension"
    },
    {
      "name": "pospicsfile",
      "type": "string",
      "default": "picnames-good.txt",
      "description": "Name of text file containing positive picture names (one per line)"
    },
    {
      "name": "negpicsfile",
      "type": "string",
      "default": "picnames-bad.txt",
      "description": "Name of text file containing negative picture names (one per line)."
    },
    {
      "name": "poswordsfile",
      "type": "string",
      "default": "words-good.txt",
      "description": "Name of text file containing positive words, one per line."
    },
    {
      "name": "negwordsfile",
      "type": "string",
      "default": "words-bad.txt",
      "description": "Name of text file containing negative words, one per line"
    },
    {
      "name": "numstim",
      "type": "integer",
      "default": 40,
      "description": "Number of stimuli per block"
    }
  ]
}