{
  "test": "changedetection",
  "version": "1.0",
  "description": "CHANGEDETECTION Test",
  "parameters": [
    {
      "name": "Tolerance",
      "type": "integer",
      "default": 30,
      "description": "How close you have to get to count as correct"
    },
    {
      "name": "UseBackground",
      "type": "boolean",
      "default": 0,
      "description": "Whether to use a faked background with many circles",
      "options": [
        0,
        1
      ]
    },
    {
      "name": "numCircles",
      "type": "integer",
      "default": 50,
      "description": "Number of circles in field.  20 is pretty easy; 50 is a bigger challenge"
    },
    {
      "name": "RadMin",
      "type": "integer",
      "default": 10,
      "description": "Minimum circle radius"
    },
    {
      "name": "RadMax",
      "type": "integer",
      "default": 30,
      "description": "Maximum circle radius"
    },
    {
      "name": "fieldwidth",
      "type": "integer",
      "default": 600,
      "description": "Field width in pixels"
    },
    {
      "name": "fieldheight",
      "type": "integer",
      "default": 400,
      "description": "field height in pixels"
    },
    {
      "name": "showTime",
      "type": "integer",
      "default": 400,
      "description": "How long to show field"
    },
    {
      "name": "ScreenTime",
      "type": "integer",
      "default": 100,
      "description": "how long to show screen"
    },
    {
      "name": "condsize",
      "type": "integer",
      "default": 5,
      "description": "Number of trials per condition"
    }
  ]
}