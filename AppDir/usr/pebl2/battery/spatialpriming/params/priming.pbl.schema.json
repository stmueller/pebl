{
  "test": "priming",
  "version": "1.0",
  "description": "PRIMING Test",
  "parameters": [
    {
      "name": "reps",
      "type": "boolean",
      "default": 1,
      "description": "How many times through the 54-trial design",
      "options": [
        0,
        1
      ]
    },
    {
      "name": "stimsize",
      "type": "integer",
      "default": 130,
      "description": "Size of the squares"
    },
    {
      "name": "delta",
      "type": "integer",
      "default": 150,
      "description": "Space between square centers"
    },
    {
      "name": "iti",
      "type": "integer",
      "default": 800,
      "description": "Inter-trial interval in ms"
    },
    {
      "name": "primedur",
      "type": "integer",
      "default": 50,
      "description": "prime duration"
    },
    {
      "name": "lag",
      "type": "integer",
      "default": 400,
      "description": "time between prime and stim"
    },
    {
      "name": "responsetype",
      "type": "string",
      "default": "choose",
      "description": "How to respond.  Select from \"mouse\",\"touchscreen\" or \"keypad\" or 'choose' to ask within the task."
    }
  ]
}