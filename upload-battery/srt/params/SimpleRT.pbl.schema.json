{
  "parameters": [
    {
      "name": "blocks",
      "type": "integer",
      "default": 2,
      "demo_override": 1,
      "description": "The number of trial blocks"
    },
    {
      "name": "blocklength",
      "type": "integer",
      "default": 50,
      "demo_override": 10,
      "description": "The number of trials per block"
    },
    {
      "name": "delayMin",
      "type": "integer",
      "default": 250,
      "description": "Smallest delay in milliseconds"
    },
    {
      "name": "delayMax",
      "type": "integer",
      "default": 2500,
      "description": "Longest delay in milliseconds"
    },
    {
      "name": "delayStep",
      "type": "integer",
      "default": 250,
      "description": "Step between smallest and longest delay"
    },
    {
      "name": "stimulus",
      "type": "string",
      "default": "X",
      "description": "Stimulus character to display"
    },
    {
      "name": "toofastThresh",
      "type": "integer",
      "default": 150,
      "description": "Response time threshold for too fast (ms)"
    },
    {
      "name": "tooslowThresh",
      "type": "integer",
      "default": 3000,
      "description": "Response time threshold for too slow (ms)"
    },
    {
      "name": "responsekey",
      "type": "string",
      "default": "X",
      "description": "Response key to use (or 'mousebutton')"
    }
  ]
}
