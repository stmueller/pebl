  gback1 <- Circle(gHomeX,gHomeY,gRadius+10,MakeColor("grey40"),1)
  AddObject(gback1,gWin)
  gback2 <- Circle(gHomeX,gHomeY,gRadius-10,MakeColor("black"),1)
  AddObject(gback2,gWin)


  ##How many revolutions per second

}


