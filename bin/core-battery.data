define ShortenPathNN(tour,pts,mat)
{

 ##nearest-neighbor method.
  cur <- First(tour)
  toura <- [cur]
  rest <- Rest(tour)
   while(Length(rest)>1)
    {

     distances <- ExtractListItems(Nth(mat,cur),rest)
     ##find closest neighbor.
     order <- Sortby(rest, distances)
     cur <- First(order)
     rest<- Rest(order)
     toura <- Append(toura,  cur)

    }

   return Merge(toura,rest)
}


