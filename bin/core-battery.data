    {
     centerX <- gVideoWidth/2
    }

   if(centerY == "")
   {
    centerY <- gVideoHeight/2
   }


   xyst <- Transpose(xys)
   xs <- First(xyst)
   ys <- Second(xyst)


   newXs <-VecSum(xs, Repeat(centerX-Mean(xs),Length(xs)))
   newYs <-VecSum(ys, Repeat(centerY-Mean(ys),Length(ys)))

  return Transpose([newXs,newYs])
}




