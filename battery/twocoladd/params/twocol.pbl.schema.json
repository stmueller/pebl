{
  "test": "twocol",
  "version": "1.0",
  "description": "TWOCOL Test",
  "parameters": [
    {
      "name": "numpractice",
      "type": "integer",
      "default": 10,
      "description": "Number of practice trials"
    },
    {
      "name": "numtrials",
      "type": "integer",
      "default": 45,
      "description": "Number of testing trials"
    },
    {
      "name": "isi",
      "type": "integer",
      "default": 250,
      "description": "ISI between tests (in ms)"
    },
    {
      "name": "timeout",
      "type": "integer",
      "default": 15000,
      "description": "Time (in ms) given to solve a problem."
    }
  ]
}