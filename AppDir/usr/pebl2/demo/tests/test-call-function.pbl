## Test calling a non-recursive user-defined function

define Start(p)
{
    Print("Testing non-recursive function call...")

    result <- Add(5, 3)
    Print("Add(5, 3) = " + result)

    result2 <- Double(10)
    Print("Double(10) = " + result2)

    Print("Test complete")
}

define Add(a, b)
{
    result <- a + b
    return(result)
}

define Double(x)
{
    result <- x * 2
    return(result)
}
