{
  "test": "PASAT",
  "version": "1.0",
  "description": "PASAT Test",
  "parameters": [
    {
      "name": "isi",
      "type": "integer",
      "default": 3000,
      "description": "Inter-stimulus interval"
    },
    {
      "name": "trailsperround",
      "type": "integer",
      "default": 20,
      "description": "Number of trials per round (between breaks)"
    },
    {
      "name": "dopractice",
      "type": "boolean",
      "default": 1,
      "description": "whether practice should be performed",
      "options": [
        0,
        1
      ]
    },
    {
      "name": "practicetrials",
      "type": "integer",
      "default": 10,
      "description": "Number of practice trials"
    },
    {
      "name": "doaudio",
      "type": "boolean",
      "default": 1,
      "description": "Whether audio should be presented (making it a true PASAT)",
      "options": [
        0,
        1
      ]
    },
    {
      "name": "dovisual",
      "type": "boolean",
      "default": 1,
      "description": "Whether visual number should also be presented.",
      "options": [
        0,
        1
      ]
    },
    {
      "name": "rounds",
      "type": "integer",
      "default": 2,
      "description": "Number of rounds to test"
    },
    {
      "name": "maxval",
      "type": "integer",
      "default": 18,
      "description": "Largest number in sequence. Should be less than maxsum, and sounds go only to 20."
    },
    {
      "name": "maxsum",
      "type": "integer",
      "default": 20,
      "description": "Largest sum to use."
    }
  ]
}