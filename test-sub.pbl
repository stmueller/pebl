## Test GetSubNum dialog

define Start(p)
{
  gWin <- MakeWindow("white")

  Print("=== Testing GetSubNum() ===")

  gSubNum <- GetSubNum(gWin)

  Print("Got subject number: " + gSubNum)

  ## Display result
  result <- EasyLabel("Subject number: " + gSubNum, gVideoWidth/2, gVideoHeight/2, gWin, 30)
  Draw()
  Wait(2000)
}
