 }



define WaitForIt()
{
  if(gInputDevice == "keyboard" or gInputDevice == "keypad")
   {
     WaitForAnyKeyPress()
   } elseif(gInputDevice=="mouse")
   {
     WaitForDownClick()
   }
}

