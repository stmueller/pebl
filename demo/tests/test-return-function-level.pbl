## Test return with } on same line vs different line - FUNCTION LEVEL ONLY

define Start(p)
{
  Print("Testing return with } placement at function level...")

  ## Test 1: return with } on SAME line
  result1 <- TestSameLine(5)
  Print("Test1 (same line): " + result1)

  ## Test 2: return with } on NEXT line
  result2 <- TestNextLine(5)
  Print("Test2 (next line): " + result2)
}

## Pattern 1: return("value") } on same line
define TestSameLine(x)
{
  result <- x * 2
  return(result) }

## Pattern 2: return("value") \n } on different lines
define TestNextLine(x)
{
  result <- x * 2
  return(result)
}
