      AddObject(lab,win)
      y <-  bottom - (Second(i)+100)/201 * (bottom-top)
      Move(lab,xline+15 + lab.width/2,y)
      tmp <- Append(tmp,lab)
   #add tick
     line <-  Line(xline,y,9,0,black)
     AddObject(line,win)
