define Insert(list, item, pos)
{
   if(pos == 0)
    {
      new <- Merge([item],list)
    } elseif(pos >= Length(list))
    {
      new <- Append(list, item)

    }else {

      new <- FlattenN([SubList(list,1,pos),
                      [item],
                      SubList(list,pos+1,Length(list))],1)
   }

  return new
}


