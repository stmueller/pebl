{
  "parameters": [
    {
      "name": "usesound",
      "type": "boolean",
      "default": true,
      "description": "Use sound feedback"
    },
    {
      "name": "numtrials",
      "type": "integer",
      "default": 50,
      "demo_override": 10,
      "description": "Number of trials"
    },
    {
      "name": "isi",
      "type": "integer",
      "default": 200,
      "description": "Inter-stimulus interval in milliseconds"
    },
    {
      "name": "responsetype",
      "type": "integer",
      "default": 0,
      "description": "Response type (0=participant choice; 1=specified keys (fjvn); 2= keypad 4512)",
      "options":[0,1,2]
    },
    {
      "name": "key1",
      "type": "string",
      "default": "F",
      "description": "Response key 1"
    },
    {
      "name": "key2",
      "type": "string",
      "default": "J",
      "description": "Response key 2"
    },
    {
      "name": "key3",
      "type": "string",
      "default": "V",
      "description": "Response key 3"
    },
    {
      "name": "key4",
      "type": "string",
      "default": "N",
      "description": "Response key 4"
    }
  ]
}
