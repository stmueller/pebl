## Simplest possible test

define Start(p)
{
    Print("Test 1: Basic print")

    result1 <- SimpleAdd(5, 3)
    Print("Test 2: SimpleAdd(5, 3) = " + result1)

    result2 <- DoubleValue(10)
    Print("Test 3: DoubleValue(10) = " + result2)

    Print("All basic tests completed!")
}

define SimpleAdd(a, b)
{
    result <- a + b
    return(result)
}

define DoubleValue(x)
{
    result <- x * 2
    return(result)
}
