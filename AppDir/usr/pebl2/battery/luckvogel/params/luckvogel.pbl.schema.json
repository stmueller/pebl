{
  "test": "luckvogel",
  "version": "1.0",
  "description": "LUCKVOGEL Test",
  "parameters": [
    {
      "name": "fieldWidth",
      "type": "integer",
      "default": 400,
      "description": "Width of field in pixels"
    },
    {
      "name": "fieldheight",
      "type": "integer",
      "default": 400,
      "description": "height of field in pixels"
    },
    {
      "name": "squaresize",
      "type": "integer",
      "default": 50,
      "description": "size of square stimuli"
    },
    {
      "name": "prestime",
      "type": "integer",
      "default": 300,
      "description": "presentation time"
    },
    {
      "name": "delaytime",
      "type": "integer",
      "default": 1000,
      "description": "delay blank"
    },
    {
      "name": "length1",
      "type": "boolean",
      "default": 1,
      "description": "size of problem",
      "options": [
        0,
        1
      ]
    },
    {
      "name": "length2",
      "type": "integer",
      "default": 2,
      "description": "size of problem"
    },
    {
      "name": "length3",
      "type": "integer",
      "default": 3,
      "description": "size of problem"
    },
    {
      "name": "length4",
      "type": "integer",
      "default": 4,
      "description": "size of problem"
    },
    {
      "name": "length5",
      "type": "integer",
      "default": 8,
      "description": "size of problem"
    },
    {
      "name": "length6",
      "type": "integer",
      "default": 12,
      "description": "size of problem"
    },
    {
      "name": "trialsperlength",
      "type": "integer",
      "default": 10,
      "description": "Number of trials for each length for both match and mismatch"
    },
    {
      "name": "givefeedback",
      "type": "boolean",
      "default": 1,
      "description": "Whether trial-by-trial feedback should be given.",
      "options": [
        0,
        1
      ]
    },
    {
      "name": "blocksize",
      "type": "integer",
      "default": 40,
      "description": "Pause for a rest after this many blocks"
    },
    {
      "name": "useaudiofeedback",
      "type": "boolean",
      "default": 1,
      "description": "Whether to give audio feedback",
      "options": [
        0,
        1
      ]
    }
  ]
}