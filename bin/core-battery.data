     FilePrint(gSumFileOut,i+ "               " + Mean(threshs)+ "      "+Mean(lasthalf)+"             "+ Last(threshs))
     PushOnEnd(meanThreshs,Mean(threshs))
   }
   FilePrint(gSumFileOut,"-----------------------------------------------")

   datalist <- Flatten([Mean(gCorr),Mean(gRTS)])
