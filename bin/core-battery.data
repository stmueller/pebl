## Repeat list enough times to make it length long.
##
define RepeatEnough(list,length)
{

   l <- Length(list)
   times <- Ceiling(length/l)
   tmp <- FlattenN(ShuffleRepeat(list,times),1)

   return SubList(tmp,1,length)
}


