## Debug test to see if Start() is entered

define Start(p)
{
    Print("DEBUG: Entered Start() function")
    Print("DEBUG: About to call MakeWindow")

    gWin <- MakeWindow("grey")

    Print("DEBUG: MakeWindow completed, about to create font")

    font <- MakeFont("DejaVuSans.ttf", 0, 24, MakeColor("black"), MakeColor("white"), 1)

    Print("DEBUG: Font created, about to create label")

    label <- MakeLabel("Test", font)

    Print("DEBUG: Label created, about to add to window")

    AddObject(label, gWin)

    Print("DEBUG: Added to window, about to move")

    Move(label, 100, 100)

    Print("DEBUG: Moved, about to draw")

    Draw()

    Print("DEBUG: Draw completed - test successful!")
}
