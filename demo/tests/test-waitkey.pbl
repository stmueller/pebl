define Start(p)
{
  gWin <- MakeWindow()
  Print("Testing WaitForListKeyPressWithTimeout")
  Print("Press left or right shift...")

  resp <- WaitForListKeyPressWithTimeout(["<lshift>", "<rshift>"], 10000)
  Print("Response: " + resp)

  if(resp == "<lshift>")
  {
    Print("Left shift pressed")
  }  elseif(resp == "<rshift>")  {
    Print("Right shift pressed")
  }  else  {
    Print("Timeout or other: " + resp)
  }
}
