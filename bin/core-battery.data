          m <- gParams.minpumps
	  ##Phase 1:
	  set1 <- Transpose([SampleBreakPointSet(gParams.life1,tps,m),Repeat(First(cols),tps),Repeat(1,tps)])
	  set2 <- Transpose([SampleBreakPointSet(gParams.life2,tps,m),Repeat(Second(cols),tps),Repeat(2,tps)])
