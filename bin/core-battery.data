 	   y <- Second(gClick)
	   
	   rating <- 21- Round(((y-400+height/2)/height)*20) 
	   ylev <- (400 + height/2) - (rating-1) * height/20
       tmpthumb <- Nth(thumbs,click)
       tmpthumb.y <- ylev
       AddObject(tmpthumb,gWin)

	   rated <- SpliceList(rated,1,click)
	   ratings <- SpliceList(ratings,rating,click) 

      } 
