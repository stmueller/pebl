#this just removes duplicated locations--false moves into walls.
define RemoveDuplicates(path)
{
  newpath <- []
  lastpos <- -1

  loop(i,path)
  {
   if(i!=lastpos)
   {
      PushOnEnd(newpath,i)
      lastpos <- i
   }
  }
  return newpath
}

