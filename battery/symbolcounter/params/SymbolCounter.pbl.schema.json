{
  "test": "SymbolCounter",
  "version": "1.0",
  "description": "SYMBOLCOUNTER Test",
  "parameters": [
    {
      "name": "numblocks",
      "type": "integer",
      "default": 3,
      "description": "Number of trial blocks"
    },
    {
      "name": "numtrials",
      "type": "integer",
      "default": 20,
      "description": "Number of total trials"
    },
    {
      "name": "runthresh",
      "type": "integer",
      "default": 10,
      "description": "Number of corrects in a row needed"
    },
    {
      "name": "givefeedback",
      "type": "boolean",
      "default": 1,
      "description": "Whether error feedback should be given",
      "options": [
        0,
        1
      ]
    },
    {
      "name": "symA1",
      "type": "string",
      "default": "#",
      "description": "A-set symbol 1 (left response)"
    },
    {
      "name": "symA2",
      "type": "string",
      "default": "@",
      "description": "A-set symbol 2"
    },
    {
      "name": "symB1",
      "type": "string",
      "default": "&",
      "description": "B-set symbol 1 (right response)"
    },
    {
      "name": "symB2",
      "type": "string",
      "default": "%",
      "description": "B-set symbol 2"
    },
    {
      "name": "toofastthresh",
      "type": "integer",
      "default": 150,
      "description": "Too fast threshold in ms"
    },
    {
      "name": "tooslowthresh",
      "type": "integer",
      "default": 3000,
      "description": "Too slow threshold in ms"
    }
  ]
}